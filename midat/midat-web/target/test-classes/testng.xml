<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Tango Automated Unit and Integration Tests" verbose="1" >

    <test name="IntegrationTests">
        <groups>
            <run>
                <include name="IntegrationTests"/>
            </run>
        </groups>
    </test>

    <test name="UnitTests">
        <groups>
            <run>
                <include name="UnitTests"/>
            </run>
        </groups>
    </test>

</suite>